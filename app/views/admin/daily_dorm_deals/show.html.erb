<% content_for :full_width_top do %>
	<%= render "shared/admin_main_nav" %>
<% end %>


<div class="std_box">
	
	<h3 class="std_box_heading">Viewing Daily Dorm Deal</h3>
	
	<div class="std_box_content">
		
		<div class="std_box_content_wrapper">
		
						<p>
			  <b>Start time:</b>
			  <%=h @daily_dorm_deal.start_time %>
			</p>

						<p>
			  <b>Type:</b>
			  <%=h @daily_dorm_deal.type_id %>
			</p>

						<p>
			  <b>Product:</b>
			  <%=h @daily_dorm_deal.product_id %>
			</p>

						<p>
			  <b>Product variation:</b>
			  <%=h @daily_dorm_deal.product_variation_id %>
			</p>

						<p>
			  <b>Title:</b>
			  <%=h @daily_dorm_deal.title %>
			</p>

						<p>
			  <b>Description:</b>
			  <%=h @daily_dorm_deal.description %>
			</p>

						<p>
			  <b>Initial qty:</b>
			  <%=h @daily_dorm_deal.initial_qty %>
			</p>

					
		</div><!-- end .std_box_content_wrapper -->
		
	</div><!-- end .std_box_content -->
	
	
	<div class="std_box_header_left"></div>
	<div class="std_box_header_right"></div>
	<div class="std_box_footer_left"></div>
	<div class="std_box_footer_right"></div>
	
</div><!-- end .std_box -->

<%= link_to '<span>Back</span>', admin_daily_dorm_deals_path, :class => 'button' %>
<%= link_to '<span>Edit</span>', edit_admin_daily_dorm_deal_path(@daily_dorm_deal), :class => 'button' %>